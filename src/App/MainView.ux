<App>	
	<JavaScript File="JavaScript/Generated/Library1.js" />

	<ClientPanel>		
		<StackPanel>
			<Text Value="{X}"><DropShadow /></Text>
			<Basic.Button Text="Click me!" Clicked="{buttonClicked}" />
			<Basic.Button Text="Delete 1" Clicked="{otherbuttonClicked}" />
			<Text Value="{obsX}" /> 
			<Text Value="{obsY}" />
			<Text Value="{obsZ}" />
			<Text Value="{obsU}" /> 
			<Text Value="{plainObservable}" />
			<Text Value="{untypedObservable}" />
			<Text Value="{untypedProjection}" />
			<StackPanel>
				<Each Items="{names}">
					<StackPanel Orientation="Horizontal"> 
						<Text Value="{name}" />
						<Text Value="{age}" />
					</StackPanel>
				</Each>
			</StackPanel> 
			<StackPanel>
				<Each Items="{kiss}">
					<Panel>
						<Text Value="{}" />
					</Panel>
				</Each>
			</StackPanel>
			<Text Value="{numNames}" />
			<Text Value="{untypedProjection}" />
			<Text Value="{counter}" />
			<ScrollView>
			<Grid ColumnCount="4">
				<Each Items="{data.tetrad.palette}">
					<DockPanel Height="80" Margin="5,0">
						<Panel DockPanel.Dock="Top" Margin="10" Height="30">
							<Rectangle Layer="Background" CornerRadius="10" Fill="#fff" />
							<Text Value="{hex}" TextAlignment="Center" Alignment="Center" />
						</Panel>

						<Rectangle Layer="Background" CornerRadius="10" Fill="{hex}" />
					</DockPanel>
				</Each>
			</Grid>
		</ScrollView>
		</StackPanel>
		
	</ClientPanel>
</App>

